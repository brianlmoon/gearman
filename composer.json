{
    "name": "moonspot/gearman-client",
    "homepage": "http://brian.moonspot.net/GearmanManager",
    "description": "Library which provides a client and worker interface for working with Gearman",
    "keywords": [
        "gearman",
        "gearman-manager"
    ],
    "type": "library",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Brian Moon",
            "homepage": "http://brian.moonspot.net"
        }
    ],
    "support": {
        "issues": "https://github.com/brianlmoon/gearman-client/issues"
    },
    "require": {
        "php": "^8.1",
        "ext-sockets": "*"
    },
    "autoload": {
        "psr-4": {
            "Moonspot\\Gearman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Net\\Gearman\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.64"
    }
}